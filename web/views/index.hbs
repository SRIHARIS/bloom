<body ng-app="bloom" ng-cloak>
	<div class="sidebar">
			<div class="intro">
					<div class="circle">
						  SH
					</div>
					<div class="message">
						 <div class="title">Bloom Labs</div>
						  <div class="subtitle">Crazy stuff starts at Heart</div>
					</div>
					<div class="clear"></div>
			</div>
			<div class="items">
					<div class="item active" link="chat">
						<a href="#/chat">
								<div class="circle">
									<i class="fa fa-comments"></i>
								</div>
								<div class="text">

											<div class="level1">Chat</div>
											<div class="detail">Last message</div>
											<div class="clear"></div>

								</div>
								<div class="clear"></div>
							</a>
					</div>
					<div class="item" link="files">
						<a href="#/files">
							<div class="circle">
								<i class="fa fa-dropbox"></i>
							</div>
							<div class="text">
									<div class="level1">Files</div>
									<div class="detail" rel="file_count"></div>
									<div class="clear"></div>
							</div>
							<div class="clear"></div>
						</a>
					</div>
					<div class="item" link="notes">
						<div class="circle">
							<i class="fa fa-heartbeat"></i>
						</div>
						<div class="text">
							<a href="#/notes">
								<div class="level1">Note</div>
								<div class="detail">hello youtube</div>
								<div class="clear"></div>
							</a>
						</div>
						<div class="clear"></div>
					</div>
			</div>
	</div>
    <div class="content">
    	<div ng-view></div>
    </div>
	<script src="https://cdn.pubnub.com/sdk/javascript/pubnub.4.0.9.js"></script>
